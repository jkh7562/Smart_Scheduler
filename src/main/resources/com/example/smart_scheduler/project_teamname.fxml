<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="113.0" prefWidth="318.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.smart_scheduler.projectteamnameController">
   <children>
      <Pane prefHeight="37.0" prefWidth="318.0" style="-fx-background-color: #c8c4ff;">
         <children>
            <HBox layoutY="9.0" prefHeight="30.0" prefWidth="163.0">
               <children>
                  <ImageView fx:id="back_image" fitHeight="24.0" fitWidth="32.0" onMouseClicked="#backimageClicked" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../backre.png" />
                     </image>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label text="팀 생성">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </Pane>
      <Pane layoutY="37.0" prefHeight="109.0" prefWidth="318.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <TextField fx:id="teamname" prefHeight="22.0" prefWidth="221.0" promptText="팀 이름을 입력하세요.">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0">
                     <graphic>
                        <ImageView fx:id="okay_button" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../check_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <padding>
                  <Insets left="30.0" top="30.0" />
               </padding>
            </HBox>
            <Label layoutX="53.0" layoutY="70.0" text="* 팀 생성시 팀장으로 자동 지정됩니다. *" textFill="RED" />
         </children>
      </Pane>
   </children>
</AnchorPane>
