<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="202.0" prefWidth="204.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.smart_scheduler.FindpasssuccessController">
   <children>
      <VBox alignment="CENTER" layoutX="1.0" prefHeight="200.0" prefWidth="204.0">
         <children>
            <Label prefHeight="33.0" prefWidth="154.0" text="초기화된 사용자의 비밀번호">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" prefHeight="38.0" prefWidth="204.0">
               <children>
                  <Label fx:id="pass_label">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <ImageView fx:id="copy_image" fitHeight="22.0" fitWidth="27.0" onMouseClicked="#copyText" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../copy.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <Label fx:id="copy_label" />
            <Button fx:id="okay_button" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#okayButtonAction" text="확인">
               <VBox.margin>
                  <Insets right="5.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
